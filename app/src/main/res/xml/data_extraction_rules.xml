<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <include domain="sharedpref" path="."/>
        <exclude domain="file" path="cache/"/>
        <exclude domain="file" path="code_cache/"/>
        <exclude domain="no_backup" path="."/>
    </cloud-backup>
    <device-transfer>
        <include domain="sharedpref" path="."/>
        <exclude domain="file" path="cache/"/>
        <exclude domain="file" path="code_cache/"/>
        <exclude domain="no_backup" path="."/>
    </device-transfer>
</data-extraction-rules>
